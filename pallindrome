class Tester {
	public static void main(String[] args) {
	int n=46763;
	int temp=n;
	int rem=0;
	int sum=0;
	while(n>0)
	{
	    rem=n%10;
	    sum=sum*10 + rem;
	    n=n/10;
	}
	if(temp==sum)
	{
	    System.out.println(temp + " is  a pallindrome");
	}
	else
	{
	     System.out.println(temp + " is not a pallindrome");
	}
	}
}


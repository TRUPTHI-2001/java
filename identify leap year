import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
	
		Scanner sc= new Scanner(System.in);
		int i,j,n,fact=1;
		System.out.println("enter the year");
		n=sc.nextInt();
		if(n%4==0)
		{
		    if(n%100!=0 || n%400==0)
		    {
		        System.out.println("leap year");
		    }
		    else
		    {
		        System.out.println("not leap year");
		    }
		}
		else
		{
		    System.out.println("not leap year");
		}
	
	
	}	
}

class Tester {
	public static void main(String[] args) {
		// Implement your code here 
		int n=371;
		int temp=371;
		int rem=0;int count=0;
		int sum=0;
		while(n>0)
		{
		    rem=n%10;
		    sum=sum+(rem*rem*rem);
		    n=n/10;
		    
		}
		if(sum==temp)
		{
		    System.out.println("yes");
		}
	
	}
}

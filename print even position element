import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
		int a[]= new int[100];
		Scanner sc= new Scanner(System.in);
		int i,j;
		System.out.println("enter the number of elements");
		int n=sc.nextInt();
		System.out.println("enter the elements");
		for(i=0;i<n;i++)
		{
		    a[i]=sc.nextInt();
		}
		for(i=0;i<n;i=i+2)
		{
		    System.out.println(a[i]);
		}
	}	
}
